import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        long N = sc.nextLong();
        sc.close();

        long largestEven = (N % 2 == 0) ? N : N - 1;
        long k = largestEven / 2;
        long sum = k * (k + 1);

        System.out.println(sum);
    }
}
