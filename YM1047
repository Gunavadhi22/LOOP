import java.io.*;
import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        long n = sc.nextLong();
        long count = 0;
        

        while (n >= 5) {
            n /= 5;
            count += n;
        }
        
        System.out.println(count);
    }
}
